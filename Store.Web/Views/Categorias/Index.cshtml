@model IEnumerable<Store.Common.Entities.CategoriaEntity>

@{
    ViewData["Title"] = "Listado";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ImageId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserM)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImageId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserM)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
